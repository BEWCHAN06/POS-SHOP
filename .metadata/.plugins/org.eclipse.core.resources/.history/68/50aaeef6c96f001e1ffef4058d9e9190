package testui;

import javax.swing.JPanel;
import javax.swing.SpringLayout;
import javax.swing.JButton;
import java.awt.event.ActionListener;
import java.awt.event.ActionEvent;
import java.awt.Font;

public class listmenu extends JPanel {

	/**
	 * Create the panel.
	 */
	public listmenu() {
		SpringLayout springLayout = new SpringLayout();
		setLayout(springLayout);
		
		JButton btnBanHang = new JButton("Bán Hàng");
		btnBanHang.setFont(new Font("Arial", Font.BOLD, 18));
		btnBanHang.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) {
			}
		});
		springLayout.putConstraint(SpringLayout.NORTH, btnBanHang, 245, SpringLayout.NORTH, this);
		springLayout.putConstraint(SpringLayout.WEST, btnBanHang, 10, SpringLayout.WEST, this);
		springLayout.putConstraint(SpringLayout.SOUTH, btnBanHang, 288, SpringLayout.NORTH, this);
		springLayout.putConstraint(SpringLayout.EAST, btnBanHang, -10, SpringLayout.EAST, this);
		add(btnBanHang);

	}
}
